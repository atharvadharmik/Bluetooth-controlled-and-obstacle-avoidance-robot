int csensor() {
  //pulse output
  digitalWrite(CtrigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(CtrigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(CtrigPin, LOW);

  long t = pulseIn(CechoPin, HIGH);//Get the pulse
  int cm = t * 0.0344 / 2; //Convert time to the distance
  return cm; // Return the values from the sensor
}

int lsensor() {
  //pulse output
  digitalWrite(A0, LOW);
  delayMicroseconds(2);
  digitalWrite(A0, HIGH);
  delayMicroseconds(10);
  digitalWrite(A0, LOW);

  long t = pulseIn(A1, HIGH);//Get the pulse
  int cm = t * 0.0344 / 2; //Convert time to the distance
  return cm; // Return the values from the sensor
}

int rsensor() {
  //pulse output
  digitalWrite(RtrigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(RtrigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(RtrigPin, LOW);

  long t = pulseIn(RechoPin, HIGH);//Get the pulse
  int cm = t * 0.0344 / 2; //Convert time to the distance
  return cm; // Return the values from the sensor
}

void setup() {
  Serial.begin (9600);
  
  //setting Motor Driver Pins
  pinMode(IN_11, OUTPUT);
  pinMode(IN_12, OUTPUT);
  pinMode(IN_13, OUTPUT);
  pinMode(IN_14, OUTPUT);

  pinMode(IN_21, OUTPUT);
  pinMode(IN_22, OUTPUT);
  pinMode(IN_23, OUTPUT);
  pinMode(IN_24, OUTPUT);

  //Setiing HC-SR04 Pins 
  pinMode(CtrigPin, OUTPUT);
  pinMode(RtrigPin, OUTPUT);
  pinMode(A0, OUTPUT);
  pinMode(CechoPin, INPUT); 
  pinMode(RechoPin, INPUT); 
  pinMode(A1, INPUT); 

  //Setting LED Pin
  pinMode(A2, OUTPUT);

  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
 
  delay(2000); // Pause for 2 seconds

  display.clearDisplay();  // Clear the buffer

// Displays DISTRESSED_EYES on the screen
  display.drawBitmap(0, 0, image_data_DISTRESSED_EYESarray, 128, 64, 1);
  display.display();

  //Setting up Random Number
  randomSeed(analogRead(A3));

}

void loop() {
  unsigned long currentMillis = millis();
  if (currentMillis - previousMillis >= interval) {
    // save the last time you blinked the LED
    previousMillis = currentMillis;

    // if the LED is off turn it on and vice-versa:
    if (ledState == LOW) {
      ledState = HIGH;
    } else {
      ledState = LOW;
    }
    // set the LED with the ledState of the variable:
    digitalWrite(A2, ledState);
  }

  display.clearDisplay();  // Clear the buffer

// Displays EYES_FRONT on the screen
  display.drawBitmap(0, 0, image_data_EYES_FRONTarray, 128, 64, 1);
  display.display();

  if(Serial.available()){
    command = Serial.read();
    if (command == 'X') {
      state = 1;
    } else if (command == 'x') {
      state = 0;
    }
    if (command == 'B') {
      back();
    } else if (command == 'F') {
      forward();
    } else if (command == 'R' && state == 1) {
      superright ();
    } else if (command == 'L' && state == 1) {
      superleft ();
    } else if (command == 'R' && state == 0) {
      right ();
    } else if (command == 'L' && state == 0) {
      left ();
    } else if (command == 'G') {
      forwardleft ();
    } else if (command == 'I') {
      forwardright ();
    } else if (command == 'H') {
      backright ();
    } else if (command == 'J') {
      backleft ();
    }  else {
      stopRobot();
    }
  }


  if(!Serial.available()){
    int cs = csensor();
    int ls = lsensor();
    int rs = rsensor();
    // Check the distance using the IF condition
    if(ls <= 13 && cs > 30){
      display.clearDisplay();  // Clear the buffer
      // Displays EYES_LEFT on the screen
      display.drawBitmap(0, 0, image_data_EYES_LEFTarray, 128, 64, 1);
      display.display();
      //stopRobot();
      Serial.println("Stop");
      delay(10);
      Serial.println("Slide Right");
      //superright();
      delay(100);
    }

    if(rs <= 13 && cs > 30){
      display.clearDisplay();  // Clear the buffer
      // Displays EYES_RIGHT on the screen
      display.drawBitmap(0, 0, image_data_EYES_RIGHTarray, 128, 64, 1);
      display.display();
      //stopRobot();
      Serial.println("Stop");
      delay(10);
      Serial.println("Slide Left");
      //superleft();
      delay(100);
    }  
    
    if (cs <= 15) {
      display.clearDisplay();  // Clear the buffer
      // Displays DISTRESSED_EYES on the screen
      display.drawBitmap(0, 0, image_data_DISTRESSED_EYESarray, 128, 64, 1);
      display.display();
      //stopRobot();
      Serial.println("Stop");
      delay(200);
      
      if(rs - ls < -2){
        Serial.println("Slide Left");
        //superleft();
        delay(500);
      }
      
      if(rs - ls > 2){
        Serial.println("Slide Right");
        //superright();
        delay(500);
      }
      
      if(abs(rs - ls) <= 2){
        randomNumber = random(3);
        if(randomNumber == 0){
          Serial.println("Turn Right");
          //right();
          delay(600);
        }
        if(randomNumber == 1){
          Serial.println("Turn Left");
          //left();
          delay(600);
        }
        if(randomNumber == 2){
          Serial.println("Turn 180ยบ");
          //right();
          delay(1200);
        }
      }
    }
    Serial.println("Forward");
    //forward();
    stopRobot();
    }
}

